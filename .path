# set PATH to include ~/bin if it exists
if [ -d "$HOME/bin" ] ; then
	export PATH="$HOME/bin:$PATH"
fi

# set PATH to include /opt/node/bin if it exists
if [ -d "/opt/node/bin" ] ; then
	export PATH="/opt/node/bin:$PATH"
fi

# set PATH to include ~/.rbenv/bin if it exists & initialize rbenv
if [ -d "$HOME/.rbenv/bin" ] ; then
	export PATH="$HOME/.rbenv/bin:$PATH"
	eval "$(rbenv init -)"
fi

if [ -d "/Applications/Postgres.app/Contents/Versions/9.3/bin" ] ; then
	export PATH="/Applications/Postgres.app/Contents/Versions/9.3/bin:$PATH"
fi
